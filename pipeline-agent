pipeline {
    agent any 
    environment{
        NAME= "ganesh"
        URL= ""
        DIR =""
    }
    stages{
        stage ("mvn clean"){
            steps {
                sh """ 
                     mvn clean install
                      

                """
                stash includes: 'target/*.jar' name: 'myjar'
            }
        }
        stage {
            agent{
                label 'agent1'
            }
            steps {
                unstash 'myjar'

                sh """ 
                    git clone ${URL}
                    cp ${DIR}/Dockerfile .
                    

                        docker build -t java1 .
                """
            }
        }
    }
}
